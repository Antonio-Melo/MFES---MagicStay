class Message
types
	public Person = User | Landlord;
instance variables
	sender: Person;
	receiver: Person;
	content: seq of char;
	inReplyTo: [Message];
operations
	public Message: Person * Person * seq of char * [Message] ==> Message 
	Message(s, r, c, m) == (
		sender := s;
		receiver := r;
		content := c;
		inReplyTo := m;
		receiver.addMessage(self);
	);
end Message