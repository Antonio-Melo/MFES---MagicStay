class Message
instance variables
	sender: Account;
	receiver: Account;
	content: seq of char;
	inReplyTo: [Message];
operations
	public Message: Account * Account * seq of char * [Message] ==> Message 
	Message(s, r, c, m) == (
		sender := s;
		receiver := r;
		content := c;
		inReplyTo := m;
		receiver.addMessage(self);
	);
end Message